<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:layout_height="match_parent" android:background="@drawable/background_pink" android:orientation="vertical">
      <LinearLayout android:id="@+id/linearLayout1" android:background="#0099cc" android:layout_width="match_parent" android:layout_height="50dip" android:gravity="center">
        <TextView android:text="Settings" android:textSize="22dip" android:textColor="#ffffff" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_weight="0" android:id="@+id/babyNameViewActivityNameLBL"></TextView>
        <LinearLayout android:id="@+id/linearLayout3" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_weight="1"></LinearLayout>
    </LinearLayout>
      <LinearLayout android:id="@+id/linearLayout2" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_weight="1" android:orientation="vertical">
          <LinearLayout android:id="@+id/linearLayout4" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_weight="1">
              <LinearLayout android:id="@+id/linearLayout6" android:layout_height="match_parent" android:layout_weight="1" android:gravity="center" android:orientation="vertical" android:layout_width="125dip">
                  <Button android:text="" android:layout_height="75dip" android:id="@+id/settingsActivityAboutBTN" android:background="@drawable/andrios" android:layout_width="75dip"></Button>
                  <TextView android:text="About" android:textColor="#000000" android:id="@+id/textView1" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
              </LinearLayout>
              <LinearLayout android:layout_width="125dip" android:orientation="vertical" android:gravity="center" android:layout_weight="1" android:layout_height="match_parent" android:id="@+id/linearLayout7">
                  <Button android:layout_width="100dip" android:background="@drawable/icon_save" android:layout_height="100dip" android:id="@+id/settingsActivitySaveBTN"></Button>
                  <TextView android:layout_width="wrap_content" android:textColor="#000000" android:text="Save" android:id="@+id/textView4" android:layout_height="wrap_content"></TextView>
              </LinearLayout>
          </LinearLayout>
          <LinearLayout android:id="@+id/linearLayout5" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_weight="1">
              <LinearLayout android:layout_width="wrap_content" android:id="@+id/linearLayout8" android:layout_height="match_parent" android:layout_weight="1" android:gravity="center" android:orientation="vertical">
                  <Button android:layout_width="100dip" android:text="Button" android:id="@+id/button3" android:layout_height="100dip"></Button>
                  <TextView android:text="TextView" android:textColor="#000000" android:id="@+id/textView3" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
              </LinearLayout>
              <LinearLayout android:layout_width="wrap_content" android:id="@+id/linearLayout9" android:layout_height="match_parent" android:layout_weight="1" android:gravity="center" android:orientation="vertical">
                  <TextView android:layout_width="wrap_content" android:textColor="#000000" android:text="Profile" android:id="@+id/textView2" android:layout_height="wrap_content"></TextView>
              </LinearLayout>
          </LinearLayout>
      </LinearLayout>
  
</LinearLayout>
