<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:layout_height="match_parent" android:background="@drawable/background_pink" android:orientation="vertical">
<LinearLayout android:id="@+id/linearLayout1" android:background="#0099cc" android:layout_width="match_parent" android:layout_height="50dip" android:gravity="center">
        <TextView android:text="Click to Set Name" android:textSize="22dip" android:textColor="#ffffff" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_weight="0" android:id="@+id/ProfileActivityNameLBL"></TextView>
        <LinearLayout android:id="@+id/linearLayout3" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_weight="1"></LinearLayout>
    </LinearLayout>
<LinearLayout android:id="@+id/linearLayout2" android:layout_height="wrap_content" android:layout_width="match_parent">
    <LinearLayout android:id="@+id/linearLayout6" android:layout_height="match_parent" android:layout_width="match_parent">
        <ImageView android:id="@+id/imageView1" android:src="@drawable/momprofile" android:layout_height="125dip" android:layout_width="125dip" android:scaleType="fitXY"></ImageView>
        <LinearLayout android:id="@+id/linearLayout7" android:layout_height="match_parent" android:layout_width="15dip" android:layout_weight="1" android:orientation="vertical">
            <LinearLayout android:layout_width="match_parent" android:id="@+id/LinearLayout02" android:gravity="center" android:layout_height="wrap_content" android:layout_weight="1">
            </LinearLayout>
            <LinearLayout android:layout_width="match_parent" android:id="@+id/LinearLayout01" android:gravity="center" android:layout_height="wrap_content" android:layout_weight="1">
                <RadioGroup android:id="@+id/radioGroup1" android:layout_height="50dip" android:orientation="horizontal" android:layout_width="wrap_content">
                    <com.andrios.pregnancyjournal.SegmentedControlButton android:text="Due" android:checked="true" android:layout_height="wrap_content" android:layout_weight="1" android:layout_width="50dip" android:id="@+id/profileActivityDueRDO"></com.andrios.pregnancyjournal.SegmentedControlButton>
                    <com.andrios.pregnancyjournal.SegmentedControlButton android:text="LMP" android:layout_height="wrap_content" android:layout_weight="1" android:layout_width="50dip" android:id="@+id/profileActivityLMPRDO"></com.andrios.pregnancyjournal.SegmentedControlButton>
                </RadioGroup>
                <TextView android:layout_width="wrap_content" android:textColor="#ffffff" android:layout_height="wrap_content" android:text="11 Sep 2012" android:layout_weight="1" android:gravity="center" android:id="@+id/profileActivityDateLBL"></TextView>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearLayout8" android:layout_width="match_parent" android:layout_height="wrap_content" android:gravity="center" android:layout_weight="1">
                <TextView android:layout_height="wrap_content" android:textColor="#ffffff" android:text="Due Date:" android:layout_weight="1" android:layout_width="wrap_content" android:gravity="center" android:id="@+id/profileActivityDateTypeLBL"></TextView>
                <TextView android:layout_height="wrap_content" android:textColor="#ffffff" android:text="07 DEC 2012" android:layout_weight="1" android:gravity="center" android:layout_width="75dip" android:id="@+id/profileActivityDate2LBL"></TextView>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
<LinearLayout android:layout_width="match_parent" android:id="@+id/linearLayout4" android:textColor="#ffffff" android:layout_height="wrap_content" android:layout_weight="1">
    <CheckBox android:text="Pregnant" android:id="@+id/checkBox1" android:layout_width="wrap_content" android:layout_height="wrap_content"></CheckBox>
</LinearLayout>
<LinearLayout android:layout_width="match_parent" android:id="@+id/linearLayout5" android:layout_height="wrap_content">
    <SeekBar android:layout_height="wrap_content" android:id="@+id/seekBar1" android:layout_width="match_parent"></SeekBar>
</LinearLayout>
</LinearLayout>
